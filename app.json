{
  "expo": {
    "name": "brutalDO",
    "slug": "brutraldo",
    "version": "0.0.6",
    "orientation": "portrait",
    "scheme": "brutaldo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": {
        "light": "./assets/ios/BrutalDoIcon-iOS-Default-1024x1024@1x.png",
        "dark": "./assets/ios/BrutalDoIcon-iOS-Dark-1024x1024@1x.png",
        "tinted": "./assets/ios/BrutalDoIcon-iOS-TintedLight-1024x1024@1x.png"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.jtmhuka33.brutraldo",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jtmhuka33.brutraldo"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/ios/BrutalDoIcon-iOS-Default-1024x1024@1x.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/AppIocn.png",
          "color": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "41242b79-42dd-4604-9ff1-41005ae1fa48"
      }
    },
    "owner": "jtmhuka33"
  }
}
